    "expo-checkbox": "~4.0.0",
    "expo-clipboard": "~7.0.1",
    "expo-constants": "~17.0.5",
    "expo-dev-client": "~5.0.12",
    "expo-font": "~13.0.2",
    "expo-image-picker": "~16.0.3",
    "expo-linking": "~7.0.5",
    "expo-location": "~18.0.7",
    "expo-router": "~4.0.16",
    "expo-secure-store": "~14.0.0",
    "expo-splash-screen": "~0.29.19",
