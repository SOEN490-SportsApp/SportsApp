{
  "name": "clientapp",
  "version": "1.0.0",
  "private": true,
  "main": "expo-router/entry",
  "type": "commonjs",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest --transformIgnorePatterns \"node_modules/(?!@toolz/allow|expo-font)/\" --env=jsdom",
    "ci-test": "jest --transformIgnorePatterns \"node_modules/(?!@toolz/allow|expo-font)/\" --env=jsdom --coverage",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "generate-report": "npm test --coverage --watchAll=false || true\n"
  },
  "jest": {
    "preset": "jest-expo",
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.ts",
      "@testing-library/jest-native/extend-expect"
    ],
    "transform": {
      "^.+\\.[tj]sx?$": "babel-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "collectCoverage": true,
    "coverageDirectory": "./coverage",
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "clover"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 60,
        "functions": 80,
        "lines": 0,
        "statements": 0
      }
    }
  },
  "dependencies": {
    "@expo/config-plugins": "~9.0.0",
